<?xml version="1.0" encoding="UTF-8"?><!-- Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is JAX-WS RI 2.1.3.3-hudson-757-SNAPSHOT. --><xs:schema xmlns:tns="http://xml.netbeans.org/schema/common" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="1.0" targetNamespace="http://xml.netbeans.org/schema/common">

<xs:complexType name="RespuestaConsulta">
<xs:sequence>
<xs:element name="estado" type="tns:EstadoTransaccion"></xs:element>
<xs:element name="tipoAcumulado" type="tns:TipoAcumulado"></xs:element>
<xs:element name="tipoSeleccion" type="tns:TipoSeleccion"></xs:element>
<xs:element name="idServicio" type="xs:int" form="unqualified" minOccurs="0"></xs:element>
<xs:element name="referenciaConsulta" type="xs:string" minOccurs="0"></xs:element>
<xs:element name="Detalles">
<xs:complexType>
<xs:sequence>
<xs:element name="Detalle" type="tns:DetalleReferencia" maxOccurs="unbounded"></xs:element>
</xs:sequence>
</xs:complexType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="EstadoTransaccion">
<xs:sequence>
<xs:element name="codigoRetorno" type="xs:int"></xs:element>
<xs:element name="descripcion" type="xs:string"></xs:element>
<xs:element name="idLogTransaccion" type="xs:long"></xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="DetalleReferencia">
<xs:sequence>
<xs:element name="referenciaPago" type="xs:string"></xs:element>
<xs:element name="monto" type="xs:decimal"></xs:element>
<xs:element name="acumulado" type="xs:decimal"></xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="RespuestaCobranza">
<xs:sequence>
<xs:element name="estado" type="tns:EstadoTransaccion"></xs:element>
<xs:element name="idTransaccion" type="xs:long" minOccurs="0"></xs:element>
<xs:element name="nroBoleta" type="xs:string" minOccurs="0"></xs:element>
<xs:element name="ticket" type="xs:string" minOccurs="0"></xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="TipoAcumulado">
<xs:restriction base="xs:string">
<xs:enumeration value="PagoAnteriores"></xs:enumeration>
<xs:enumeration value="PagoTotal"></xs:enumeration>
<xs:enumeration value="PagoIndependiente"></xs:enumeration>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="TipoSeleccion">
<xs:restriction base="xs:string">
<xs:enumeration value="RadioButton"></xs:enumeration>
<xs:enumeration value="CheckBox"></xs:enumeration>
</xs:restriction>
</xs:simpleType>
</xs:schema>